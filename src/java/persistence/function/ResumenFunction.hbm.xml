<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "classpath://org/hibernate/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <sql-query callable="true" name="ResumenReportesFunction">
        { ? = call get_resumen_reportes(?,?,?,?) }
    </sql-query>
    <sql-query callable="true" name="ResumenMaterialesFunction">
        { ? = call get_resumen_materiales(?,?,?,?) }
    </sql-query>
</hibernate-mapping>
