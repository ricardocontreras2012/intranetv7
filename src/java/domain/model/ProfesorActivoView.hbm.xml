<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "classpath://org/hibernate/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-update="false" name="domain.model.ProfesorActivoView" table="PROFESOR_ACTIVO">
    <id name="rut" type="int">
      <column name="CPRO_PROF_RUT" precision="8" scale="0"/>
      <generator class="assigned"/>
    </id>
    <property name="carrera" type="java.lang.Integer">
      <column name="CCAR_COD_CAR" precision="5" scale="0"/>
    </property>
    <property name="mencion" type="java.lang.Integer">
      <column name="CCAR_COD_MEN" precision="5" scale="0"/>
    </property>
    <property name="depto" type="java.lang.Integer">
      <column name="DEPTO" precision="3" scale="0"/>
    </property>
    <property name="facultad" type="java.lang.Integer">
      <column name="FACULTAD" precision="3" scale="0"/>
    </property>
    <property name="email" type="string">
      <column length="200" name="PROF_EMAIL"/>
    </property>
  </class>
</hibernate-mapping>
