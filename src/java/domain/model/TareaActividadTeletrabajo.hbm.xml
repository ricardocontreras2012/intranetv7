<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="domain.model.TareaActividadTeletrabajo" table="TAREA_ACTIVIDAD_TELETRABAJO">
        <composite-id class="domain.model.TareaActividadTeletrabajoId" name="id">
            <key-property name="tatelRut" type="int">
                <column name="TATEL_RUT" precision="8" scale="0"/>
            </key-property>
            <key-property name="tatelFecha" type="date">
                <column name="TATEL_FECHA" />
            </key-property>
            <key-property name="tatelTarea" type="int">
                <column name="TATEL_TAREA" precision="9" scale="0"/>
            </key-property>
        </composite-id>
        <many-to-one class="domain.model.ActividadTeletrabajo" fetch="select" insert="false" name="actividadTeletrabajo" update="false">
            <column name="TATEL_RUT" not-null="true" precision="8" scale="0"/>
            <column name="TATEL_FECHA" not-null="true"/>
        </many-to-one>
        <property name="tatelDes" type="string">
            <column length="500" name="TATEL_DES"/>
        </property>
         <property name="tatelEvidencia" type="string">
            <column length="1" name="TATEL_EVIDENCIA"/>
        </property>
    </class>
</hibernate-mapping> 
